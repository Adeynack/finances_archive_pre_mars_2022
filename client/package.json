{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "clean": "rimraf ../build",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "yarn lint:types && yarn lint:style && yarn lint:format",
    "lint:types": "tsc --noEmit",
    "lint:style": "eslint './src/**/*.{tsx,ts}' --max-warnings 0",
    "lint:format": "prettier --list-different \"src/**/*.{css,json}\"",
    "gen": "yarn graphql:schema && yarn graphql:types",
    "graphql:schema": "apollo client:download-schema --endpoint=http://localhost:3000/graphql",
    "graphql:types": "rm -rf src/generated && apollo codegen:generate --target typescript --outputFlat src/generated --passthroughCustomScalars"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@apollo/client": "^3.2.5",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "graphql": "^15.4.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-scripts": "4.0.0",
    "react-use-form-library": "^0.0.16",
    "redux": "^4.0.5",
    "redux-database": "^0.0.20",
    "styled-components": "^5.2.1",
    "typescript": "^4.0.3",
    "web-vitals": "^0.2.4"
  },
  "devDependencies": {
    "apollo": "^2.31.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-graphql": "^4.0.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "prettier": "^2.1.2",
    "rimraf": "^3.0.2"
  }
}
